  <class_name>SCEAddCentralIceStorage</class_name>
  <display_name>SCE Add Central Ice Storage</display_name>
  <description>Adds an ice storage tank and controls to existing chilled water loop for the purpose of thermal energy storage.</description>
  <modeler_description>The measure allows users attach ice thermal energy storage (ITS) to an existing chilled water loop for the purpose of exploring load flexibility potential for a given building. </modeler_description>
  <arguments>
    <argument>
      <name>stor_cap</name>
      <display_name>Thermal Storage Capacity (ton-hours)</display_name>
      <description>Energy capacity of Ice thermal storage in ton-hours</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>300</default_value>
    </argument>
    <argument>
      <name>charge_hours</name>
      <display_name>Battery Charge Hours</display_name>
      <description>If Storage Control Mode = 'Scheduled', enter daily times for battery charging, using 24-hour format. Applies every day in the full run period.</description>
      <type>String</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>HH:MM - HH:MM</default_value>
    </argument>
  </arguments>
